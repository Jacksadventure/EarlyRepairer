digraph G {
    #########
    # STYLE #
    #########

    # draw edges behind nodes (not working for subgraphs)
    outputorder=edgesfirst;

    # allow compound nides
    compound=true;

    # layout from left to right
    rankdir="LR";

    # specify font for graph
    fontname = "Sans";

    edge [color="#106DF1"];

    node [color="#106DF1",
          fillcolor="#F4F3EA",
          fontcolor="#106DF1",
          style="filled",
          shape="rectangle"];

    ###################
}
